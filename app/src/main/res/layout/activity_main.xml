<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/bg_login"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:text="I am home"
        android:id="@+id/text_home"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/calon"
        android:layout_width="121dp"
        android:layout_height="154dp"
        android:layout_marginTop="120dp"
        android:src="@drawable/calon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.241"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lihat Calon"
        app:layout_constraintEnd_toEndOf="@+id/calon"
        app:layout_constraintStart_toStartOf="@+id/calon"
        app:layout_constraintTop_toBottomOf="@id/calon" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voting"
        app:layout_constraintEnd_toEndOf="@+id/voting"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="@+id/voting"
        app:layout_constraintTop_toBottomOf="@id/voting" />

    <ImageView
        android:id="@+id/voting"
        android:layout_width="121dp"
        android:layout_height="154dp"
        android:layout_marginTop="120dp"
        android:src="@drawable/calon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toEndOf="@+id/calon"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/hasil"
        android:layout_width="121dp"
        android:layout_height="154dp"
        android:layout_marginTop="328dp"
        android:src="@drawable/calon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.241"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/profil"
        android:layout_width="121dp"
        android:layout_height="154dp"
        android:layout_marginTop="328dp"
        android:src="@drawable/calon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.824"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>